{
  "name": "chat-window",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev -p ${PORT:-3000}",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "start": "next start -p ${PORT:-3000}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.3",
    "@papercups-io/browser": "0.0.4-beta.0",
    "@sentry/react": "^6.5.1",
    "antd": "^4.4.2",
    "dayjs": "^1.10.6",
    "framer-motion": "^2.3.0",
    "next": "^11.0.0",
    "phoenix": "^1.5.4",
    "rc-upload": "^3.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-emoji-render": "^1.2.4",
    "react-markdown": "^8.0.4",
    "remark-breaks": "^2.0.0",
    "superagent": "^5.3.1",
    "theme-ui": "^0.11.2",
    "tinycolor2": "^1.4.1",
    "use-composed-ref": "^1.1.0",
    "use-latest": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@types/phoenix": "^1.5.0",
    "@types/react": "^17.0.80",
    "@types/superagent": "^4.1.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^5.4.5"
  }
}
